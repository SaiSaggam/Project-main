<div class="row" name="CVROVERVIEW_FT">
    <div class="col-xs-12 col-sm-12 col-lg-6" style="padding-top:10px;">
        <div class="panel panel-info">
            <div class="panel-heading panel-blue" ng-click="showOOPInfo()">
                <h3 class="panel-title entire-panel-heading">Annual Deductible <i ng-if="!isOOPInfo" class="fa fa-plus float-right"></i><i ng-if="isOOPInfo" class="fa fa-minus float-right"></i></h3>
            </div>
            <div class="panel-body" slide-toggle="isOOPInfo">
                <div ng-if="deducatableOOPLoading" class="loading-label">
                    <span><i class="fa fa-spinner fa-spin fa-2x" aria-hidden="true"></i>Loading...</span>
                </div>
                <div class="row" ng-if="deducatableOOPErr">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <div>
                            <div class="col-lg-12 col-md-12 col-sm-12 panel-body">
                                <div class="searchFormErrorMsgBox" name="ERRORMSG_FT">{{deducatableOOPErrInfo}}</div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Member Annual Deductible with ytd and remaining -->
                <table class="annual-deductable-table" ng-if="!deducatableOOPLoading && !deducatableOOPErr && isMemberSearch">
                    <thead style=" text-decoration: underline;">
                        <tr>
                            <td class="padding-heading-inn "><b>In Network</b></td>
                            <td class=""><b>Family</b> </td>
                            <td class=""><b>Individual</b> </td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="border-bottom">
                            <td class="padding-data-inn ">Maximum:</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.INN.familyDeductibleMax}}</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.INN.individualDeductibleMax}}</td>
                        </tr>
                        <tr class="border-bottom">
                            <td class="padding-data-inn ">Applied:</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.INN.familyDeductibleYTD}}</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.INN.individualDeductibleYTD}}</td>
                        </tr>
                        <tr class="border-bottom">
                            <td class="padding-data-inn ">Remaining:</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.INN.familyDeductibleRem}}</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.INN.individualDeductibleRem}}</td>
                        </tr>
                    </tbody>
                    <thead style=" text-decoration: underline; padding-top:30px">
                        <tr>
                            <td class="padding-heading-inn "><b>Out of Network</b></td>
                            <td class=" "><b>Family</b> </td>
                            <td class=" "><b>Individual</b> </td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="border-bottom">
                            <td class="padding-data-inn ">Maximum:</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.OON.familyDeductibleMax}}</td>
                            <td class=" padding-left-0-5em">$ {{deducatableOOPData.OON.individualDeductibleMax}}</td>
                        </tr>
                        <tr class="border-bottom">
                            <td class="padding-data-inn ">Applied:</td>
                            <td class="padding-left-0-5em ">$ {{deducatableOOPData.OON.familyDeductibleYTD}}</td>
                            <td class="padding-left-0-5em ">$ {{deducatableOOPData.OON.individualDeductibleYTD}}</td>
                        </tr>
                        <tr class="border-bottom">
                            <td class="padding-data-inn ">Remaining:</td>
                            <td class="padding-left-0-5em ">$ {{deducatableOOPData.OON.familyDeductibleRem}}</td>
                            <td class="padding-left-0-5em ">$ {{deducatableOOPData.OON.individualDeductibleRem}}</td>
                        </tr>
                    </tbody>
                </table>
                <!-- Group Annual Deductible table with only Primary values -->
                <table class="annual-deductable-table"  ng-if="!deducatableOOPLoading && !deducatableOOPErr && isGroupSearch">
                    <thead style=" text-decoration: underline; ">
                        <tr>
                            <td class="padding-heading-inn "><b>In Network</b></td>
                            <td class=""><b>Family</b> </td>
                            <td class=""><b>Individual</b> </td>
                        </tr>
                    </thead>
                    <tbody style="height:20px">
                        <tr>
                            <td class="padding-data-inn ">Maximum:</td>
                            <td class="padding-left-0-5em">$ {{riderData.basePlan.INNPrimaryFamilyDeductibleMaximum}}</td>
                            <td class="padding-left-0-5em ">$ {{riderData.basePlan.INNPrimaryIndividualDeductibleMaximum}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
